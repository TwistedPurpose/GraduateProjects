
@{
    ViewBag.Title = "Hub";
}








<div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
            <textarea class="notes-text-edit" rows="12"></textarea>
        </div>
    </div>

    <div class="row">
        <div class="col-sm-8 map">
            <div class="google-map-canvas" id="map">
            </div>
        </div>

        <div class="col-sm-4">

            <div class="row">
                <div class="col-sm-4">
                    <button class="btn btn-default">Upload Map</button>

                    <button class="btn btn-default">Set Root Map</button>
                </div>
            </div>

            <div class="row">
                <div class="col-sm-4">
                    <button class="btn btn-default">Add Character</button>
                    <div data-bind="foreach:characters"></div>
                </div>

            </div>

            <div class="row">
                <div class="col-sm-4">
                    <button class="btn btn-default">Add An Item</button>
                </div>
            </div>
        </div>
    </div>
</div>

