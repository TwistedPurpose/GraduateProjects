<div class="modal fade planner-modal" id="addExistingCharacterModal" data-bind="modal: existingCharacterModal">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Add Existing Character</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body" data-bind="with: existingCharacterVM">
                <div data-bind="foreach:CharacterList">
                    <div class="checkbox">
                        <input class="col-sm-1" type="checkbox" data-bind="checkedValue: Id, checked:$parent.SelectedCharacters">
                        <span data-bind="text:Name"></span>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" data-bind="click:associateCharacters">Save changes</button>
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>