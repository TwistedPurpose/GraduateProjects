<div class="modal fade" id="existingItemModal" data-bind="modal: existingItemsModal">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Add Existing Character</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body" data-bind="with: existingItemsVM">
                <select multiple="multiple" height="5" width="100%" data-bind="options:CompleteItemList, selectedOptions:SelectedItems, optionsText: 'Name', optionsValue: 'Id'"> </select>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" data-bind="click:associateItems">Save</button>
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>