<div class="modal fade planner-modal" id="existingMapModal" data-bind="modal: existingMapModal">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Set Base Map</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body" data-bind="with: existingMapVM">
                <select width="100%" data-bind="options:MapList, optionsText:'Name', value:SelectedMap, optionsValue: 'Id'"> </select>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" data-bind="click:saveNewBaseMap">Save</button>
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>