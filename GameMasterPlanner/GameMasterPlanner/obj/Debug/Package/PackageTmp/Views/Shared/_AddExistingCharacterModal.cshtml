<div class="modal fade" id="addExistingCharacterModal" data-bind="modal: showAddExistingCharacterModal">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Add Existing Character</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body" data-bind="with: addExistingCharacterVM">
                <select multiple="multiple" height="5" data-bind="options:CharacterList, selectedOptions:SelectedCharacters"> </select>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" data-bind="click:saveCharacter">Save changes</button>
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>