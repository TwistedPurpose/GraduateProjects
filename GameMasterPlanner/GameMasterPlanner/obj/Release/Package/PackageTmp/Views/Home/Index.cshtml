@{
    ViewBag.Title = "Index";
}

<div>
    <div>
        <textarea data-bind="value:newCampaginName"></textarea>
        <textarea data-bind="value:newCampaginHistory"></textarea>

        <button data-bind="click:addCampagin">Add Campaign</button>
    </div>


    <table>
        <thead>
            <tr>
                <th>Campaign Name</th>
                <th>History</th>
            </tr>
        </thead>
        <tbody data-bind="foreach:campaignList">
            <tr>
                <td data-bind="text:name"></td>
                <td data-bind="text:history"></td>
            </tr>
        </tbody>

    </table>


</div>
@Scripts.Render("~/bundles/campaign")